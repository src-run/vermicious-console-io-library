{
  "name"        : "src-run/vermicious-console-io-library",
  "license"     : "MIT",
  "type"        : "library",
  "homepage"    : "https://src.run/vermicious-console-io-library",
  "description" : "basic set of Symfony console Input/Output/Style implementations",
    "support" : {
    "email"  : "rmf@src.run",
    "issues" : "https://src.run/vermicious-console-io-library/issues",
    "source" : "https://src.run/vermicious-console-io-library/git"
  },
  "keywords" : [
    "vermicious-console-io-library", "vermicious", "console", "io", "library", "rmf", "src-run"
  ],
  "authors" : [
    {
      "name"    : "Rob Frawley 2nd",
      "email"   : "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role"    : "Project Lead"
    }
  ],
  "require" : {
    "php"             : "^5.6||^7.0",
    "symfony/console" : "^2.7||^2.8||^3.0"
  },
  "require-dev" : {
    "phpunit/phpunit"                     : "^5.0",
    "codacy/coverage"                     : "^1.0",
    "satooshi/php-coveralls"              : "^1.0",
    "src-run/augustus-reflection-library" : "^0.6",
    "symfony/var-dumper"                  : "^2.7||^2.8||^3.0"
  },
  "autoload" :     {
    "psr-4" : {
      "SR\\Console\\Style\\"  : "lib/Style/",
      "SR\\Console\\Input\\"  : "lib/Input/",
      "SR\\Console\\Output\\" : "lib/Output/",
      "SR\\Console\\Std\\"    : "lib/Std/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Console\\Tests\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config": {
    "bin-dir" : "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master" : "0.1-dev"
    }
  }
}
