{
  "name": "src-run/vermicious-console-io-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/vermicious-console-io-library",
  "description": "basic set of Symfony console Input/Output/Style implementations",
    "support": {
    "email": "rmf@src.run",
    "issues": "https://src.run/vermicious-console-io-library/issues",
    "source": "https://src.run/vermicious-console-io-library/git"
  },
  "keywords": [ "vermicious-console-io-library", "vermicious", "console", "io", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require" : {
    "php": "~7.1",
    "src-run/augustus-exception-library": "~0.7",
    "symfony/console": "~3.0"
  },
  "require-dev": {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~2.0",
    "phpunit/phpunit": "~5.4",
    "satooshi/php-coveralls": "~1.0",
    "sllh/php-cs-fixer-styleci-bridge": "~2.1",
    "symfony/yaml": "~3.0"
  },
  "autoload":     {
    "psr-4": {
      "SR\\Console\\Input\\": "lib/Input/",
      "SR\\Console\\Output\\": "lib/Output/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Console\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  }
}
